<h1>Consoles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consoles.each do |console| %>
      <tr>
        <td><%= console.name %></td>
        <td><%= console.price %></td>
        <td><%= console.quantity %></td>
        <td><%= link_to 'Show', console %></td>
        <td><%= link_to 'Edit', edit_product_path(console) %></td>
        <td><%= link_to 'Destroy', console, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

